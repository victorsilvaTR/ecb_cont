Attribute VB_Name = "frmPlantillaRemuneracion"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim lstrSql As String

Private Sub cmdImportar_Click()
    If Me.tdbgListadoDiario.Columns(0).Value <> vbNullString Then
        frmManAsientosContables.RecibirDatos "TipoAsientoRemuneracion", Me.tdbgListadoDiario.Columns(0).Value, "", ""
        Unload Me
    End If
End Sub

Private Sub cmdSalir_Click()
    Unload Me
End Sub

Private Sub Form_Load()
    GetAllLibros
    GetAllPeriodos
End Sub

Private Sub GetAllLibros()
    lstrSql = "SELECT clo.Lib_cTipoLibro, clo.Lib_cDescripcion FROM dbo.CNT_LIBRO_OPERA CLO " & _
              "WHERE CLO.Emp_cCodigo = '" & gsEmpresa & "' AND CLO.Pan_cAnio = '" & gsAnio & "' AND CLO.Lib_cTipoLibro = '" & lsLibroDiario & "'"
    LlenarComboAddItem Me.tdbcLibro, lstrSql, False
End Sub

Private Sub GetAllPeriodos()
    Dim intIndice As Integer
    
    For intIndice = 0 To 11
        Me.tdbcPeriodo.AddItem Format(intIndice + 1, "00") & ";" & UCase(MonthName(intIndice + 1))
    Next
    
    Me.tdbcPeriodo.Bookmark = 0
    Me.tdbcPeriodo.ListField = "column1"
    Me.tdbcPeriodo.BoundColumn = "column0"
    
End Sub

Private Sub GetAllVoucher()
    
    lstrSql = "SELECT cav.Ase_nVoucher AS Voucher, cav.Ase_cGlosa AS Glosa FROM dbo.CNC_ASIENTO_VOUCHER CAV " & _
              "WHERE CAV.Emp_cCodigo = '" & gsEmpresa & "' AND CAV.Lib_cTipoLibro = '" & Me.tdbcLibro.BoundText & "' AND CAV.Per_cPeriodo = '" & Me.tdbcPeriodo.BoundText & "' AND CAV.Pan_cAnio = '" & gsAnio & "'"
                      
    Set Me.tdbgListadoDiario.DataSource = Nothing
                      
    Dim ObjFuncion As New ClsFuncionesExecute
    Dim rsVoucher As ADODB.Recordset
    
    Set rsVoucher = ObjFuncion.fRetornaRS(lstrSql)
    If Not rsVoucher Is Nothing Then
        Me.tdbgListadoDiario.DataSource = rsVoucher
    End If
                  
End Sub

Public Property Let Grupo(ByVal Grupo As String)
     gsGrupo = Grupo
End Property

Private Sub tdbcPeriodo_SelChange(Cancel As Integer)
    GetAllVoucher
End Sub
