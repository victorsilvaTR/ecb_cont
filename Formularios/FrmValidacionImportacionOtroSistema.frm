Private Sub GetValidacion()
    
    pblnValidacion = True
    
    lstrSql = "DELETE dbo.CNT_REPORTE_IMPORTACION " & _
              "WHERE Emp_cCodigo = '" & gsEmpresa & "'"
    ObjFuncion.pEjecutaSQL (lstrSql)
    
    lstrSql = "spCn_ValidacionImportacionOtroSistema @Accion = 'IMPORTACION', " & _
              "@Emp_cCodigo = '" & gsEmpresa & "', " & _
              "@Pan_cAnio = '" & gsAnio & "', " & _
              "@Par_cEnt = '" & pstrEntidad & "', " & _
              "@Par_cTca = '" & pstrTipoCambio & "', " & _
              "@Par_cApe = '" & pstrApertura & "', " & _
              "@Par_cCom = '" & pstrCompra & "', " & _
              "@Par_cVen = '" & pstrVenta & "', " & _
              "@Par_cCin = '" & pstrIngreso & "', " & _
              "@Par_cCeg = '" & pstrEgreso & "', " & _
              "@Par_cPlan = '" & pstrDiario & "', " & _
              "@User = '" & gsUsuario & "', " & _
              "@Periodo = '" & gsPeriodo & "'"
    ObjFuncion.pEjecutaSQL (lstrSql)
    
    Dim rsImportacion As ADODB.Recordset
    
    lstrSql = "SELECT * FROM dbo.CNT_REPORTE_IMPORTACION CRI " & _
              "WHERE CRI.Emp_cCodigo = '" & gsEmpresa & "'"
    Set rsImportacion = New ADODB.Recordset
    Set rsImportacion = ObjFuncion.fRetornaRS(lstrSql)
    
    Dim intIndex As Integer
    Dim strMensaje As String
    strMensaje = vbNullString
    
    If rsImportacion.RecordCount > 0 Then
       pblnValidacion = False
       
       Dim matriz_fecha(3) As Variant
       Screen.MousePointer = vbHourglass

       matriz_fecha(0) = "@Accion;REPORTE;True"
       matriz_fecha(1) = "@Emp_cCodigo;" & gsEmpresa & ";True"
       matriz_fecha(2) = "@EMPRESA;" & gsEmpresaNom & ";True"
       matriz_fecha(3) = "@RUC;" & gsRUC & ";True"

       Dim formulas(0) As Variant

       AbreReporteParam gsDSN, Me, rutaReportes & "RptImportacionDatos.rpt", crptToWindow, "Reporte de Importacin de Datos", "", matriz_fecha(), formulas()
        
       Screen.MousePointer = vbNormal
'       MsgBox strMensaje, vbCritical, "Sistema ECB-Cont"
    Else
        pblnValidacion = True
        Unload Me
        
        frmPrcImportarDatosXLS.chkOpcion(0).Value = pstrEntidad
        frmPrcImportarDatosXLS.chkOpcion(1).Value = pstrTipoCambio
        frmPrcImportarDatosXLS.chkOpcion(2).Value = pstrApertura
        frmPrcImportarDatosXLS.chkOpcion(3).Value = pstrCompra
        frmPrcImportarDatosXLS.chkOpcion(4).Value = pstrVenta
        frmPrcImportarDatosXLS.chkOpcion(5).Value = pstrIngreso
        frmPrcImportarDatosXLS.chkOpcion(6).Value = pstrEgreso
        frmPrcImportarDatosXLS.chkOpcion(7).Value = pstrDiario
        frmPrcImportarDatosXLS.Show
        frmPrcImportarDatosXLS.Procesar
    End If
    
End Sub

