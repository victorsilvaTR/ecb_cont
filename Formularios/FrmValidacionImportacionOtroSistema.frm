    If Me.pstrTipoCambio = 1 Then Me.SSTab1.Tab = 1
    If Me.pstrApertura = 1 Then Me.SSTab1.Tab = 4
    If Me.pstrCompra = 1 Then Me.SSTab1.Tab = 2
    If Me.pstrVenta = 1 Then Me.SSTab1.Tab = 3
    If Me.pstrIngreso = 1 Then Me.SSTab1.Tab = 5
