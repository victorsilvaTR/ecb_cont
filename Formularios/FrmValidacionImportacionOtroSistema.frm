Private Function GetExistenciaTablas(ByVal NombreTabla As String) As Boolean
    
    lstrSql = "IF EXISTS(SELECT 1 FROM sysobjects WHERE type = 'U' AND name = '" & NombreTabla & "') " & _
        "BEGIN SELECT 1 AS Valor End Else BEGIN SELECT 0 AS Valor END "
        
    Dim rsExistencia As ADODB.Recordset
    Set rsExistencia = New ADODB.Recordset
    Set rsExistencia = ObjFuncion.fRetornaRS(lstrSql)
    
    If (rsExistencia("Valor").Value = 1) Then
        GetExistenciaTablas = True
    Else
        GetExistenciaTablas = False
    End If
    
End Function

